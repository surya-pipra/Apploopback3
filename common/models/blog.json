{
  "name": "blog",
  "base": "CFBasePersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "cfuser": {
      "type": "belongsTo",
      "model": "cfuser",
      "foreignKey": "userId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comments",
      "foreignKey": "blogId",
      "primaryKey": "id"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "find",
      "accessType": "EXECUTE"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create",
      "accessType": "EXECUTE"
    }
  ],
  "methods": {}
}